layer {
  name: "adacos_add_m_scale"
  type: "AdaCosAddmScale"
  bottom: "fc6"
  bottom: "label"
  top: "fc6_margin_scale"
  adacos_add_m_scale_param {
    m: 0.5
    num_classes: 10575
  }
}


layer {
  name: "softmax_loss"
  type: "SoftmaxWithLoss"
  bottom: "fc6_margin_scale"
  bottom: "label"
  top: "softmax_loss"
  loss_weight: 1
}

layer {
  name: "accuracy-t"
  type: "Accuracy"
  bottom: "fc6"
  bottom: "label"
  top: "accuracy-t"
  include {
    phase: TRAIN
  }
}

layer {
  name: "accuracy"
  type: "Accuracy"
  bottom: "fc6_margin_scale"
  bottom: "label"
  top: "accuracy"
  include {
    phase: TRAIN
  }
}
