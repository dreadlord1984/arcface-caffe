############### Arc-Softmax Loss ##############

layer {
  name: "fc6"
  type: "InnerProduct"
  bottom: "norm_fc5"
  top: "fc6"
  param {
    lr_mult: 1
  }
  inner_product_param {
    num_output: 10575
    normalize: true
    weight_filler {
      type: "xavier"
    }
    bias_term: false
  }
}

